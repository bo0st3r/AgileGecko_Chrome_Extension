{
  "name": "Agile Gecko",
  "version": "1.0",
  "description": "Chrome extension that makes reaching a CoinGecko coin's page faster.",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "webRequest",
    "webNavigation",
    "tabs",
    "browsingData",
    "clipboardRead"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://api.coingecko.com/api/v3/*"
      ],
      "js": [
        "scripts/util.js"
      ],
      "run_at": "document_end"
    }
  ]
}



